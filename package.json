      "@react-google-maps/api": "^1.9.7",
      "axios": "^0.18.0",
      "bcryptjs": "^2.4.3",
      "compression": "^1.7.4",
      "express": "^4.16.3",
      "express-session": "^1.17.1",
      "google-map-react": "^1.1.7",
      "if-env": "^1.0.4",
      "mongoose": "^5.3.16",
      "mysql2": "^2.1.0",
      "passport": "^0.4.1",
      "passport-local": "^1.0.0",
      "prop-types": "^15.7.2",
